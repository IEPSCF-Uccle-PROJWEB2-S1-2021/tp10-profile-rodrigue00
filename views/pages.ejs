<%- include('header'); -%>
    <div class="container">
        <h1>
            <%= title %>
        </h1>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Prenom</th>
                    <th scope="col">Nom</th>
                    <th scope="col">Code postal</th>

                </tr>
            </thead>
            <tbody>
                <% longueur.forEach((item) => { %>
                    <tr>
                        <td>
                            <%= Nom %>
                        </td>
                        <td class="text-right">
                            <%= Prenom %>
                        </td>
                        <td class="text-right">
                            <%= code %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <%- include('footer') -%>